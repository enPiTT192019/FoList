<?xml version="1.0" encoding="utf-8"?>
<vector
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:name="vector"
    android:width="24dp"
    android:height="24dp"
    android:viewportWidth="24"
    android:viewportHeight="24">

    <group
        android:name="horizontal_group"
        android:pivotX="12"
        android:pivotY="12"
        >

        <path
            android:name="horizontal"
            android:pathData="M 4 12 L 20 12"
            android:strokeColor="#000000"
            android:strokeWidth="2"/>

    </group>

    <group
        android:name="vertical_group"
        android:pivotX="12"
        android:pivotY="12"
        >

        <path
            android:name="vertical"
            android:pathData="M 12 4 L 12 20"
            android:strokeColor="#000000"
            android:strokeWidth="2"/>

    </group>
</vector>
